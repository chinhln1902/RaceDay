package model;

/**
 * An interface which gave the common methods for message.
 * 
 * @author Chinh Le
 * @version 11/08/2018
 */
public interface Message {
    /**
     * the metod which get the current time stamp.
     * @return current time stamp.
     */
    String getTimeStamp();
    
    /**
     * the method which get the type of message.
     * @return the type of message.
     */
    String getType();
    
    /**
     * combine all the parts of message to a string.
     * @return a string of message.
     */
    String toString();
}
